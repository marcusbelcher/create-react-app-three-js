{
  "name": "create-react-app-three-js",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "three": "^0.103.0"
  },
  "scripts": {
    "copyThree": "ncp './node_modules/three/build/three.min.js' './public/js/three.min.js' && ncp './node_modules/three/examples/js/renderers/CSS3DRenderer.js' './public/js/CSS3DRenderer.js'",
    "start": "yarn copyThree && react-scripts start",
    "build": "yarn copyThree && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "ncp": "^2.0.0"
  }
}
